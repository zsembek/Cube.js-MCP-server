{
    "mcpServers": {
        "cubejs": {
            "command": "uvx",
            "args": [
                "git+https://github.com/zsembek/Cube.js-MCP-server.git"
            ],
            "env": {
                "CUBEJS_API_BASE_URL": "http://localhost:4000/cubejs-api/v1",
                "CUBEJS_API_TOKEN": "your_api_token"
            }
        }
    }
}